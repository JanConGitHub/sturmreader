{
	"clickable_minimum_required": "6.12.2",
	"builder": "cmake",
	"qt_version": "5.12",
	"dependencies_target": [
		"intltool",
		"libpoppler-qt5-dev",
		"libquazip5-dev",
		"libpodofo-utils",
		"qtwebengine5-dev",
		"qtwebengine5-dev-tools"
	],
	"dependencies_host": [
		"gettext"
	],
	"install_bin": [
		"/usr/bin/podofoimg2pdf"
	],
	"install_lib": [
		"/usr/lib/${ARCH_TRIPLET}/libquazip5.so.*",
		"/usr/lib/libpodofo.so.*"
	],
	"default": "clean build install launch logs"
}

